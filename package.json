{
  "name": "tmg",
  "description": "Timer generator",
  "version": "0.4.1",
  "repository": "andrepolischuk/tmg",
  "dependencies": {
    "bootstrap-gh-pages": "^0.2.1",
    "tmg": "^0.4.1"
  },
  "devDependencies": {
    "browserify": "^10.2.3",
    "postcss": "^4.1.11",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2"
  },
  "scripts": {
    "prebuild": "rm -rf build/*",
    "build": "npm run build-js && npm run build-css",
    "build-js": "browserify index.js > build/index.js",
    "build-css": "postcss --use postcss-import index.css > build/index.css"
  },
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT"
}
